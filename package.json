{
  "name": "onlineshop",
  "version": "1.0.0",
  "description": "Online shop design based on the MERN stack",
  "main": "server.js",
  "scripts": {
    "install:app": "npm install && npm run install:client",
    "install:client": "npm install --prefix client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kombajn27/OnlineShop.git"
  },
  "keywords": [
    "Online shop",
    "e-commerce",
    "MERN"
  ],
  "author": "FreeFrogs, Kombajn27",
  "license": "ISC",
  "homepage": "https://github.com/Kombajn27/OnlineShop#readme",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "concurrently": "^5.0.2",
    "config": "^3.2.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.2"
  }
}
